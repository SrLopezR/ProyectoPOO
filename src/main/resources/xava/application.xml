<?xml version = "1.0" encoding = "ISO-8859-1"?>

<!DOCTYPE application SYSTEM "dtds/application.dtd">

<application name="Proyecto">

	<!--
	A default module for each entity is assumed with the
	controllers on <default-module/>
	-->
	<default-module>
		<controller name="Typical"/>
	</default-module>

    <!-- Módulo para Programación de Servicios -->
    <module name="ProgramacionServicios">
        <model name="ProgramacionServicios"/>
        <controller name="ProgramacionController"/>
    </module>

    <!-- Módulo para Inventario -->
    <module name="Inventario">
        <model name="Inventario"/>
        <controller name="InventarioController"/>
    </module>


</application>
